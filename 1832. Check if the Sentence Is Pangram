class Solution {
    public boolean checkIfPangram(String sentence) {
        HashSet<Character> check = new HashSet<>();
        if(sentence.length() >= 26)
        {
            for(int i = 0 ; i < sentence.length() ; i++)
            {
                char c = sentence.charAt(i);
                check.add(c);
            }
            return check.size() == 26;
        }
        else{
            return false;
        }
        
        
    }
}
